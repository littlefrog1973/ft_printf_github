{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang-12 build active file",
            "command": "/usr/bin/clang-12",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "-Wall",
                "-Werror",
                "-Wextra",
                "${workspaceFolder}/main.c",
                "${workspaceFolder}/libftprintf.a",
                "${workspaceFolder}/ft_printf.h",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
